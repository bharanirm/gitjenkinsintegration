pipeline {
    agent {
        label 'WindowsNode'
      }
       
    stages {
        stage('Checkout') {
           
            steps {
                 echo "Running ${env.BUILD_ID} on ${env.JENKINS_URL}"
                    git url: 'https://github.com/bharanirm/gitjenkinsintegration.git', branch: 'master', credentialsId: '6153a06b50ce4c3cf2b0317abcb88c92e8a4adb4'
                   }
        }
        stage('Load Grrovy Script'){
            script {
            code = load 'myfile.groovy'
            code.example1()
            code.example2()
            }
        }
       /* stage('Execute Groovy'){
            script {
            code.example1()
            }
        } */
        stage('Env Testing') {
            /* agent { label 'WindowsNode' } */
            steps {
               script {
                   def fields = env.getEnvironment()
                   fields.each {
                        key, value -> println("${key} = ${value}");
                    }
                    println(env.PATH)
                    println(env.PATBUILD_ID)
                     }
            }
        }
          stage('Env') {
            steps {
                script{
                println("Running ${env.BUILD_ID} on ${env.JENKINS_URL}")
                }
                
            }
        }              
        } 
}
       