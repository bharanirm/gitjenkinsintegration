                   }
        }
        stage('Env Testing') {
            /* agent { label 'WindowsNode' } */
            steps {
               script {
                   def fields = env.getEnvironment()
                   fields.each {
                        key, value -> println("${key} = ${value}");
                    }
 
                    println(env.PATH)
               }
            }
        }
        stage('Load Grrovy Script'){
            code = load 'myfile.groovy'
        }
        stage('Execute Groovy'){
            code.example1()
        }


            } 
            code.example2()                          
        } 
        def pipeline = load 'myfile.groovy'
